e: String,\n      default: 'Item'\n    }\n  },\n  data () {\n    return {\n      full: false\n    }\n  },\n\n  computed: {\n    formTitle () {\n      __log(this.$store.state)\n      return this.$t((this.editedIndex === -1 ? 'new-item' : 'edit-item'), { item: this.routeName })\n    },\n    activatorText () {\n      return this.$t('new-item', { item: this.routeName })\n    }\n  },\n\n  methods: {\n    screenListener (e) {\n      // __log(e.target);\n      this.full = e.target.fullScreen\n    },\n    formReference () {\n      return this.id + '-form'\n    },\n\n    confirmCallback () {\n      const self = this\n      this.$refs[this.formReference()].saveForm((res) => {\n        self.closeModal()\n      })\n    },\n\n    save () {\n      console.log(\n        'save clicked',\n        this.formObject\n      )\n    }\n  }\n}\n</script>\n\n<style>\n\n</style>\n"],"mappings":"AAwEA,OAAOA,MAAK,MAAO,uBAAsB;AACzC,SAASC,UAAS,QAAS,UAAS;AAEpC,eAAe;EACbC,MAAM,EAAE,CAACD,UAAU,CAAC;EACpBE,UAAU,EAAE;IACV,SAAS,EAAEH;EACb,CAAC;EACDI,KAAK,EAAE;IACLC,SAAS,EAAE;MACTC,IAAI,EAAEC,MAAM;MACZ,WAAS;IACX;EACF,CAAC;EACDC,IAAG,WAAAA,KAAA,EAAK;IACN,OAAO;MACLC,IAAI,EAAE;IACR;EACF,CAAC;EAEDC,QAAQ,EAAE;IACRC,SAAQ,WAAAA,UAAA,EAAK;MACXC,KAAK,CAAC,IAAI,CAACC,MAAM,CAACC,KAAK;MACvB,OAAO,IAAI,CAACC,EAAE,CAAE,IAAI,CAACC,WAAU,KAAM,CAAC,IAAI,UAAS,GAAI,WAAW,EAAG;QAAEC,IAAI,EAAE,IAAI,CAACZ;MAAU,CAAC;IAC/F,CAAC;IACDa,aAAY,WAAAA,cAAA,EAAK;MACf,OAAO,IAAI,CAACH,EAAE,CAAC,UAAU,EAAE;QAAEE,IAAI,EAAE,IAAI,CAACZ;MAAU,CAAC;IACrD;EACF,CAAC;EAEDc,OAAO,EAAE;IACPC,cAAa,WAAAA,eAAGC,CAAC,EAAE;MACjB;MACA,IAAI,CAACZ,IAAG,GAAIY,CAAC,CAACC,MAAM,CAACC,UAAS;IAChC,CAAC;IACDC,aAAY,WAAAA,cAAA,EAAK;MACf,OAAO,IAAI,CAACC,EAAC,GAAI,OAAM;IACzB,CAAC;IAEDC,eAAc,WAAAA,gBAAA,EAAK;MACjB,IAAMC,IAAG,GAAI,IAAG;MAChB,IAAI,CAACC,KAAK,CAAC,IAAI,CAACJ,aAAa,EAAE,CAAC,CAACK,QAAQ,CAAC,UAACC,GAAG,EAAK;QACjDH,IAAI,CAACI,UAAU,EAAC;MAClB,CAAC;IACH,CAAC;IAEDC,IAAG,WAAAA,KAAA,EAAK;MACNC,OAAO,CAACC,GAAG,CACT,cAAc,EACd,IAAI,CAACC,UAAS,CAChB;IACF;EACF;AACF"},"metad